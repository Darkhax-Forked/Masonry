{
"multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "masonry:block/stonecarvedwritingpost" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "masonry:block/stonecarvedwritingside", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "masonry:block/stonecarvedwritingside", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "masonry:block/stonecarvedwritingside", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "masonry:block/stonecarvedwritingside", "y": 270, "uvlock": true }
        }
    ]
}
